    {
        "id": "a4c334ec0802f7b1",
        "type": "function",
        "z": "bf283ad4cf60086f",
        "name": "montar_query",
        "func": "// --- Recebe os géneros selecionados da dropdown ---\nlet genres = msg.payload;\n\n// Garante que é sempre um array (caso o utilizador selecione só um)\nif (!Array.isArray(genres)) {\n    genres = [genres];\n}\n\n// Se nenhum género for selecionado, devolve uma query genérica\nif (genres.length === 0) {\n    node.warn(\"Nenhum género selecionado — mostrando todos.\");\n    msg.topic = `\n        SELECT g.genre,\n               ROUND(AVG(t.popularity),2) AS avg_popularity\n        FROM tracks t\n        JOIN albums a ON t.album_id = a.id\n        JOIN artists ar ON a.artist_id = ar.id\n        JOIN artist_genres g ON ar.id = g.id\n        WHERE t.popularity IS NOT NULL\n        GROUP BY g.genre\n        ORDER BY avg_popularity DESC\n        LIMIT 10;\n    `;\n    return msg;\n}\n\n// Converte o array de géneros para formato SQL: 'pop','rock','edm'\nconst genreList = genres.map(g => `'${g}'`).join(\",\");\n\n// Cria a query filtrada pelos géneros escolhidos\nmsg.topic = `\nSELECT \n    g.genre,\n    ROUND(AVG(t.popularity),2) AS avg_popularity\nFROM tracks t\nJOIN albums a ON t.album_id = a.id\nJOIN artists ar ON a.artist_id = ar.id\nJOIN artist_genres g ON ar.id = g.id\nWHERE g.genre IN (${genreList})\n  AND t.popularity IS NOT NULL\nGROUP BY g.genre\nORDER BY avg_popularity DESC;\n`;\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
