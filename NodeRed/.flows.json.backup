    {
        "id": "ae02e54410d9afdb",
        "type": "function",
        "z": "bf283ad4cf60086f",
        "name": "gerar_script_sql",
        "func": "// O valor selecionado da dropdown (ex: \"valence\", \"energy\", etc.)\nlet atributo = msg.payload;\n\n// Monta o script SQL dinâmico\nlet sql = `\nSELECT\nSUBSTR(release_date, 1, 7) AS month,\n  AVG(${atributo}) AS avg_attribute\nFROM tracks t\nJOIN albums al ON t.album_id = al.id\nGROUP BY month\nORDER BY month\n`;\n\n// Envia o SQL para o próximo nó (SQL node)\nmsg.topic = sql;\nreturn msg;\n\n\n\n",
