        "func": "// --- Recebe géneros selecionados da dropdown ---\nlet genres = msg.payload;\n\n// Garante que é sempre um array\nif (!Array.isArray(genres)) {\n    genres = [genres];\n}\n\n// Se nenhum género for selecionado → mostra todos\nif (genres.length === 0) {\n    node.warn(\"Nenhum género selecionado — mostrando todos os géneros.\");\n\n    msg.topic = `\n        SELECT \n          strftime('%Y-%m', a.release_date) AS release_month,\n          ROUND(AVG(t.popularity),2) AS avg_popularity,\n          g.genre\n        FROM tracks t\n        JOIN albums a ON t.album_id = a.id\n        JOIN artists ar ON a.artist_id = ar.id\n        JOIN artist_genres g ON ar.id = g.id\n        WHERE t.popularity IS NOT NULL\n        GROUP BY g.genre, release_month\n        ORDER BY release_month;\n    `;\n    return msg;\n}\n\n// Converte os géneros selecionados para formato SQL: 'pop','rock','edm'\nconst genreList = genres.map(g => `'${g}'`).join(\",\");\n\n// Query filtrada pelos géneros selecionados\nmsg.topic = `\nSELECT \n  strftime('%Y-%m', a.release_date) AS release_month,\n  ROUND(AVG(t.popularity),2) AS avg_popularity,\n  g.genre\nFROM tracks t\nJOIN albums a ON t.album_id = a.id\nJOIN artists ar ON a.artist_id = ar.id\nJOIN artist_genres g ON ar.id = g.id\nWHERE g.genre IN (${genreList})\n  AND t.popularity IS NOT NULL\nGROUP BY g.genre, release_month\nORDER BY release_month;\n`;\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 1420,
        "y": 920,
